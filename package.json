{
  "name": "doddle-classifier-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--max-old-space-size=4096 && npm run serve:base",
    "start-heroku": "node server.js",
    "serve:base": "ng serve --port 9000 --host 0.0.0.0 --ssl --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "serve:ric": "set NODE_OPTIONS=--max-old-space-size=4096 && npm run serve:base -- --configuration=ric-development",
    "build": "npm run build:development",
    "build:development": "set NODE_OPTIONS=--max-old-space-size=4096 && ng build --configuration=development",
    "build:test": "set NODE_OPTIONS=--max-old-space-size=4096 && ng build --configuration=test",
    "build:production": "set NODE_OPTIONS=--max-old-space-size=4096 && ng build --configuration=production",
    "deploy:dev": "npm run build:development && node ftp-deploy dev",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@tensorflow/tfjs": "^2.8.2",
    "core-js": "^3.8.1",
    "express": "^4.17.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.16",
    "@types/uikit": "^3.3.1",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5"
  }
}
